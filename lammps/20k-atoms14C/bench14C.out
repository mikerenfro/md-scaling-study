LAMMPS (31 Mar 2017)
variable        T equal 300.0
variable        p equal   1.0
variable        Tdamp equal  100.0
variable        pdamp equal 1000.0
variable	new string "20k_infiniband_gpu"

units           real
neigh_modify    delay 0 every 1 check yes

atom_style      full
bond_style      harmonic
angle_style     charmm
dihedral_style  charmm
improper_style  harmonic

pair_style      lj/charmm/coul/long 10 12
pair_modify     mix arithmetic
kspace_style    pppm 1e-4

read_data       benchmark.data
  orthogonal box = (-29.7267 -27.2901 -29.7267) to (29.7267 27.2901 29.7267)
  2 by 1 by 7 MPI processor grid
  reading atoms ...
  19605 atoms
  reading velocities ...
  19605 velocities
  scanning bonds ...
  4 = max bonds/atom
  scanning angles ...
  6 = max angles/atom
  scanning dihedrals ...
  18 = max dihedrals/atom
  scanning impropers ...
  2 = max impropers/atom
  reading bonds ...
  13294 bonds
  reading angles ...
  7504 angles
  reading dihedrals ...
  1866 dihedrals
  reading impropers ...
  102 impropers
  4 = max # of 1-2 neighbors
  9 = max # of 1-3 neighbors
  19 = max # of 1-4 neighbors
  21 = max # of special neighbors

special_bonds   charmm
fix             1 all shake 1.0e-6 500 0 m 1.0 b 47 a 106
  121 = # of size 2 clusters
  55 = # of size 3 clusters
  28 = # of size 4 clusters
  6321 = # of frozen angles
fix             2 all npt temp $T $T ${Tdamp} iso $p $p ${pdamp}
fix             2 all npt temp 300 $T ${Tdamp} iso $p $p ${pdamp}
fix             2 all npt temp 300 300 ${Tdamp} iso $p $p ${pdamp}
fix             2 all npt temp 300 300 100 iso $p $p ${pdamp}
fix             2 all npt temp 300 300 100 iso 1 $p ${pdamp}
fix             2 all npt temp 300 300 100 iso 1 1 ${pdamp}
fix             2 all npt temp 300 300 100 iso 1 1 1000

thermo          10000
thermo_style    multi
thermo_modify   flush yes
timestep        2.0

restart         10000 benchmark.rst
dump            1 all dcd 25000 benchmark.dcd
dump_modify     1 unwrap yes

run             50000
