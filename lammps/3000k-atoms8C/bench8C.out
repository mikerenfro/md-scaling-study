fix             2 all npt temp $T $T ${Tdamp} z $p $p ${pdamp}
fix             2 all npt temp 300 $T ${Tdamp} z $p $p ${pdamp}
fix             2 all npt temp 300 300 ${Tdamp} z $p $p ${pdamp}
fix             2 all npt temp 300 300 100 z $p $p ${pdamp}
fix             2 all npt temp 300 300 100 z 1 $p ${pdamp}
fix             2 all npt temp 300 300 100 z 1 1 ${pdamp}
fix             2 all npt temp 300 300 100 z 1 1 1000

thermo          1000
thermo_style    multi
thermo_modify   flush yes
timestep        2.0

restart         1000 lmp-benchmark.rst
dump            1 all dcd 5000 lmp-benchmark.dcd
dump_modify     1 unwrap yes

run             10000
